{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/App/domain/dto": ["./src/app/domain/dto"],
      "@/App/domain/entity": ["./src/app/domain/entity"],
      "@/App/domain/service": ["./src/app/domain/service"],

      "@/App/infrastructure/http": ["./src/app/infrastructure/http"],
      "@/App/infrastructure/repository": ["./src/app/infrastructure/repository"],

      "@/App/presentation/design": ["./src/app/presentation/design"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts"],
  "exclude": ["node_modules"]
}
